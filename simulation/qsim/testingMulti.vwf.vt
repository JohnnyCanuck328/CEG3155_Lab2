`timescale 1 ps/ 1 ps
module multiplier4bit_vlg_sample_tst(
	multiplicand,
	multiplier,
	sampler_tx
);
input [3:0] multiplicand;
input [3:0] multiplier;
output sampler_tx;

reg sample;
time current_time;
always @(multiplicand or multiplier)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

